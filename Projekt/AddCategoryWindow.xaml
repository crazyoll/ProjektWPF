<Window x:Class="Projekt.AddCategoryWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Projekt"
        mc:Ignorable="d"
        Title="Dodaj Kategorię" SizeToContent="Height" Width="263.178" Height="108.04">
    <Border>
        <Grid Margin="0,0,3,0">
            <StackPanel>
                <Button Margin="4" Name="SaveButton" HorizontalAlignment="Right" Click="AddCategoryToDB">Zapisz</Button>
                <Border Margin="3" Grid.Row="1" BorderBrush="Black" BorderThickness="1">
                    <TextBox Name="CategoryNameBar" Margin="4" Padding="3" Text="Nazwa..." />
                </Border>
                
            </StackPanel>
        </Grid>
    </Border>
</Window>
